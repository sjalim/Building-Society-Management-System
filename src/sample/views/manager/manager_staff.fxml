<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ManagerStaffController">
   <children>
      <TextField fx:id="search_text_field" layoutX="317.0" layoutY="37.0" prefHeight="46.0" prefWidth="553.0" promptText="Search" styleClass="search-text-area" stylesheets="@../../styles/Staff.css" />
      <ImageView fx:id="search_image_view" fitHeight="31.0" fitWidth="31.0" layoutX="814.0" layoutY="45.0" onMouseClicked="#handleSearch" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/search.png" />
         </image>
      </ImageView>
      <Button fx:id="add_button" layoutX="263.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleAdd" prefHeight="95.0" prefWidth="139.0" text="Add">
         <font>
            <Font name="Trebuchet MS" size="30.0" />
         </font>
      </Button>
      <Button fx:id="update_text_field" layoutX="555.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleUpdate" prefHeight="95.0" prefWidth="139.0" text="Update">
         <font>
            <Font name="Trebuchet MS" size="30.0" />
         </font>
      </Button>
      <Button fx:id="delete_text_field" layoutX="838.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleDelete" prefHeight="95.0" prefWidth="139.0" text="Delete">
         <font>
            <Font name="Trebuchet MS" size="30.0" />
         </font>
      </Button>
      <TableView fx:id="table" layoutX="13.0" layoutY="366.0" prefHeight="331.0" prefWidth="1175.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="227.0" text="Name" />
          <TableColumn fx:id="type" minWidth="0.0" prefWidth="242.0" text="Type" />
            <TableColumn fx:id="shift_start" prefWidth="209.0" text="Shift Start" />
            <TableColumn fx:id="shift_end" minWidth="0.0" prefWidth="260.0" text="Shift End" />
            <TableColumn fx:id="mobile" prefWidth="311.0" text="Mobile" />
        </columns>
      </TableView>
      <ComboBox fx:id="type_combo_box" layoutX="56.0" layoutY="239.0" prefHeight="25.0" prefWidth="270.0" promptText="Type" />
      <Label fx:id="search_label" layoutX="43.0" layoutY="297.0" prefHeight="46.0" prefWidth="747.0" text="The search result of &quot;search&quot;">
         <font>
            <Font name="Trebuchet MS" size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
