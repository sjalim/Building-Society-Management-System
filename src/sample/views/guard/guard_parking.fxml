<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<ScrollPane hbarPolicy="NEVER" prefHeight="500.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.GuardParkingController">
   <content>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #009688;">
         <children>
            <Pane layoutX="-2.0" prefHeight="800.0" prefWidth="251.0" style="-fx-background-color: #002323;">
               <children>
                  <JFXButton fx:id="filter_button" layoutX="25.0" layoutY="374.0" prefHeight="53.0" prefWidth="207.0" styleClass="btn" stylesheets="@../../styles/guardParking.css" text="Filter">
                     <font>
                        <Font name="Trebuchet MS" size="25.0" />
                     </font>
                  </JFXButton>
                  <JFXTimePicker fx:id="in_time_timepicker" defaultColor="BLACK" layoutX="7.0" layoutY="167.0" prefHeight="28.0" prefWidth="115.0" promptText="In Time" styleClass="pickers" stylesheets="@../../styles/guardParking.css" />
                  <JFXTimePicker fx:id="out_time_timepicker" defaultColor="BLACK" layoutX="130.0" layoutY="167.0" prefHeight="28.0" prefWidth="115.0" promptText="Out Time" styleClass="pickers" stylesheets="@../../styles/guardParking.css" />
                  <JFXDatePicker fx:id="tracker_date_datepicker" defaultColor="BLACK" layoutX="29.0" layoutY="245.0" prefHeight="28.0" prefWidth="200.0" promptText="Tracker date" styleClass="pickers" stylesheets="@../../styles/guardParking.css" />
               </children>
            </Pane>
            <TableView fx:id="table" layoutX="303.0" layoutY="104.0" prefHeight="463.0" prefWidth="850.0" styleClass="table" stylesheets="@../../styles/guardParking.css">
              <columns>
                <TableColumn fx:id="date_col" prefWidth="148.0" text="Date" />
                <TableColumn fx:id="car_num_col" prefWidth="159.0" text="Car Number" />
                  <TableColumn fx:id="car_model_coi" prefWidth="151.0" text="Car Model" />
                  <TableColumn fx:id="in_time_col" prefWidth="198.0" text="In Time" />
                  <TableColumn fx:id="out_time_col" prefWidth="193.0" text="Out Time" />
                  <TableColumn fx:id="allocation_flat_col" prefWidth="136.0" text="Allocated Flat" />
              </columns>
               <effect>
                  <DropShadow />
               </effect>
            </TableView>
            <JFXTextField fx:id="search_text_field" focusColor="BLACK" labelFloat="true" layoutX="563.0" layoutY="33.0" prefHeight="35.0" prefWidth="421.0" promptText="Search">
               <font>
                  <Font size="20.0" />
               </font>
            </JFXTextField>
         </children></AnchorPane>
   </content>
</ScrollPane>
